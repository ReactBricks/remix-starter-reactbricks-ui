{
  "name": "remix-starter-reactbricks-ui",
  "private": true,
  "description": "A simple starter to get started quickly with React Bricks and Remix",
  "version": "1.3.7",
  "author": "Matteo Frana <f2@f2.net>",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"node -r dotenv/config node_modules/.bin/remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "start-local": "node -r dotenv/config node_modules/.bin/remix-serve build"
  },
  "dependencies": {
    "@remix-run/react": "^1.14.3",
    "@remix-run/serve": "^1.14.3",
    "@tailwindcss/forms": "^0.5.3",
    "dotenv": "^16.0.3",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-bricks": "3.9.0-beta.7",
    "react-dom": "^18.2.0",
    "react-slick": "^0.29.0",
    "remix": "^1.14.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.14.3",
    "@types/react": "^18.0.30",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^7.6.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
